{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "ESNext",
    "target": "es5",
    "lib":[
      "ES2017", "ES2015","DOM"
    ],
    "jsx": "react",
    "strictNullChecks": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "moduleResolution": "Node",
    "allowJs": false,
    "strictFunctionTypes": true,
  },
  "exclude": ["bak"]
}